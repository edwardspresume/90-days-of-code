lt = document.querySelector(\"#results\");\n\n    // Clear First\n    result.textContent = \"\";\n\n    pets.forEach(pet => {\n      console.log(pet);\n      const div = document.createElement(\"div\");\n      div.classList.add(\"card\", \"card-body\", \"mb-3\");\n\n      div.innerHTML = `\n      <div class='row'>\n        <div class='col-sm-6'>\n            <h4>${pet.name.$t} ${pet.age.$t}</h4>\n            <p class='text-secondary'>${pet.breeds.breed.$t}</p>\n            <p>${pet.contact.address1.$t} ${pet.contact.city.$t} ${\n        pet.contact.state.$t\n      } ${pet.contact.zip.$t}</p>\n            <ul class='list-group'>\n                <li class='list-group-item'>Phone: ${pet.contact.phone.$t}</li>\n                ${\n                  pet.contact.email.$t\n                    ? `<li class='list-group-item'>Email: ${\n                        pet.contact.email.$t\n                      }`\n                    : \"\"\n                }\n   \n        </div>\n\n        <div class='col-sm-6'>\n\n        </div>\n      </div>\n      `;\n\n      result.appendChild(div);\n    });\n  };\n}\n\n/* ==========================================================================\n                                 Render the animals\n   ========================================================================== */\n"},"lineCount":null}},"hash":"b0c55134291c28b5c1d9fd48694e7f18","cacheData":{"env":{}}}